{
  "editor.cursorBlinking": "smooth",
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.lineNumbers": "on",
  "editor.lineHeight": 30,
  "explorer.openEditors.visible": 0,
  "editor.quickSuggestions": {
    "comments": true
  },
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    120,
    160
  ],
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "extensions.ignoreRecommendations": false,
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.exclude": {
    ".git": false
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.path": "C:\\tools\\Git\\bin\\git.exe",
  "intelephense.completion.backslashPrefix": false,
  "html.format.enable": false,
  "javascript.format.enable": false,
  "json.format.enable": false,
  "typescript.format.enable": false,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "php-cs-fixer.rules": "@PSR2",
  "php-cs-fixer.config": "~/.vscode/.php_cs.laravel",
  "php-cs-fixer.lastDownload": 1586859000277,
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "storage/framework/views": true
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Hybrid Next (Gray Background)",
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  }
}
