{
  "css.validate": false,
  "diffEditor.renderSideBySide": true,
  "discord.detailsIdle": "Idling",
  "discord.detailsEditing": "Editing",
  "discord.detailsDebugging": "Debugging",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
    "source.fixAll.markdownlint": true
  },
  "editor.cursorBlinking": "smooth",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "relative",
  "editor.lineHeight": 30,
  "explorer.openEditors.visible": 0,
  "editor.quickSuggestions": {
    "comments": true
  },
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120, 160],
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "emmet.triggerExpansionOnTab": true,
  "eslint.validate": ["javascript", "javascriptreact", "vue"],
  "eslint.alwaysShowStatus": true,
  "extensions.ignoreRecommendations": false,
  "files.autoSave": "afterDelay",
  "files.eol": "\n",
  "files.exclude": {
    ".git": false
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.path": "C:\\tools\\Git\\bin\\git.exe",
  "html.format.enable": false,
  "intelephense.completion.backslashPrefix": false,
  "json.format.enable": false,
  "scss.validate": false,
  "typescript.format.enable": false,
  "php.suggest.basic": false,
  "php-cs-fixer.config": "~/.vscode/.php_cs.laravel",
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "php-cs-fixer.lastDownload": 1614610415866,
  "php-cs-fixer.rules": "@PSR2",
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "storage/framework/views": true
  },
  "stylelint.enable": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
  "vetur.format.enable": false,
  "vetur.validation.template": false,
  "vetur.validation.script": false,
  "vetur.validation.style": false,
  "vim.leader": "<space>",
  "vim.useCtrlKeys": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Hybrid Next (Gray Background)",
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  }
}
